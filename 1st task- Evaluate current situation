df = pd.DataFrame(data)  # Create DataFrame from the data

# Step 3: Calculate Push Up Utilization Rate
df['push_up_utilization_rate'] = df['revenue_from_push_ups'] / df['number_of_listings']

# Step 4: Sort categories by Push Up Utilization Rate
sorted_df = df.sort_values(by='push_up_utilization_rate', ascending=False)

# Step 5: Calculate Average Revenue per Listing
df['avg_revenue_per_listing'] = df['revenue_from_push_ups'] / df['number_of_listings']

# Step 6: Plot the relationship
plt.scatter(df['push_up_utilization_rate'], df['avg_listing_price_eur'])
plt.title('Push Up Utilization Rate vs. Average Listing Price')
plt.xlabel('Push Up Utilization Rate')
plt.ylabel('Average Listing Price (EUR)')
plt.grid(True)
plt.show()

# Display the sorted DataFrame
sorted_df


